# -*- shell-script -*- :mode=shellscript:

ISABELLE_IMPORTER_JAR="$COMPONENT/target/scala-2.12/importer-isabelle-assembly-0.1.0-SNAPSHOT.jar"
echo "$ISABELLE_IMPORTER_JAR"

if [ -f "$ISABELLE_IMPORTER_JAR" ]
then
  classpath "$ISABELLE_IMPORTER_JAR"
  isabelle_scala_tools "de.qaware.findfacts.dumpimporter.Tools"
else
  echo "### Missing $ISABELLE_IMPORTER_JAR - use sbt assembly"
fi
